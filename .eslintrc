{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
	"es6": true,
    "browser": true,
	"node": true,
  },
  "rules": {
    "comma-dangle": 0,
	"import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
	"new-cap": [1,  {"newIsCap": true, "capIsNew": true}],
	"no-console": 0,
  }
}
